	<parameter name="start_date" isForPrompting="true" class="java.lang.String"/>
	<parameter name="end_date" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select distinct
t_billing_receipt.billing_receipt_number as billing_receipt_number
,t_visit.visit_hn    as hn
,f_patient_prefix.patient_prefix_description || ' ' || t_patient.patient_firstname ||' '|| t_patient.patient_lastname AS name_pt
,t_visit.visit_patient_age as patient_age
,substr(t_visit.visit_begin_visit_time,1,10) as visit_date
,t_order.order_common_name as order_name
,t_order.order_qty    as qty
,(t_order.order_price * t_order.order_qty) as price


from
t_visit INNER JOIN t_patient
    ON t_patient.t_patient_id = t_visit.t_patient_id
LEFT JOIN f_patient_prefix
    ON t_patient.f_patient_prefix_id=f_patient_prefix.f_patient_prefix_id
INNER JOIN t_billing
    ON t_visit.t_visit_id = t_billing.t_visit_id
INNER JOIN t_billing_receipt
    ON t_billing.t_billing_id = t_billing_receipt.t_billing_id
INNER JOIN t_order
	ON t_visit.t_visit_id = t_order.t_visit_id
INNER JOIN b_item
	ON t_order.b_item_id = b_item.b_item_id


where ((t_visit.f_visit_status_id <>'4')
        AND ((t_order.f_order_status_id <>'0')
		AND (t_order.f_order_status_id <> '3') )
and t_billing_receipt.billing_receipt_paid > 0 --รายการรับชำระจำนวนที่จ่ายต้อง >0
and substr(t_visit.visit_begin_visit_time,1,10) between $P{start_date} and $P{end_date} )
order by t_billing_receipt.billing_receipt_number]]></queryString>

