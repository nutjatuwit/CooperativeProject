	<parameter name="start_date" class="java.lang.String"/>
	<parameter name="end_date" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
b_site.site_full_name AS site_name
,b_visit_clinic.visit_clinic_description as clinic
,case when (sum(q1.s_sold)  > 0 ) then (sum(q1.s_sold)) else 0 end AS s_sold
,case when (sum(q1.s_sold)  > 0 ) then (sum(q1.s_sold)) else 0 end AS total

FROM b_site,b_visit_clinic

LEFT JOIN
(select
--t_visit.b_visit_clinic_id AS b_visit_clinic_id
--,b_visit_clinic.visit_clinic_description
t_visit.t_patient_id AS t_visit
, t_diag_icd10.b_visit_clinic_id AS b_visit_clinic_id
,CASE  WHEN ((t_health_family.f_person_rank_id='4')
 AND ( t_patient.f_sex_id='1'OR  t_patient.f_sex_id='2'))
THEN  1 ELSE 0 END  AS s_sold

FROM t_patient
    INNER JOIN t_visit
      ON t_patient.t_patient_id = t_visit.t_patient_id
    INNER JOIN t_health_family
      on t_health_family.t_health_family_id = t_patient.t_health_family_id
    inner join f_person_rank
      on t_health_family.f_person_rank_id = f_person_rank.f_person_rank_id
INNER JOIN t_diag_icd10 ON t_visit.t_visit_id = t_diag_icd10.diag_icd10_vn
and t_diag_icd10.f_diag_icd10_type_id = '1' and t_diag_icd10.diag_icd10_active = '1'

WHERE substring(t_visit.visit_financial_discharge_time,1,10) Between $P{start_date} AND $P{end_date}
and t_visit.f_visit_type_id = '1'
and t_health_family.f_person_rank_id ='4'
and t_visit.f_visit_status_id <> '4'
and t_diag_icd10.b_visit_clinic_id in ('1313085667988','1313113216096','1316934835220','1310000000007','1310000000006','1317636542907','131115283993493179','1319229321299','131141618431279141','131115280541408418')
group by
t_visit.t_patient_id
,t_diag_icd10.b_visit_clinic_id
,t_health_family.f_person_rank_id
,t_patient.f_sex_id

)AS q1


ON b_visit_clinic.b_visit_clinic_id=q1.b_visit_clinic_id
where b_visit_clinic.b_visit_clinic_id in ('1313085667988','1313113216096','1316934835220','1310000000007','1310000000006','1317636542907','131115283993493179','1319229321299','131141618431279141','131115280541408418')


group by
b_site.site_full_name
,b_visit_clinic.visit_clinic_description
,b_visit_clinic.b_visit_clinic_id

order by
b_visit_clinic .b_visit_clinic_id asc]]>
	</queryString>
