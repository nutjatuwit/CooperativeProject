	<queryString>
		<![CDATA[select
            physical_therapy_order.seq as physical_therapy_seq
            ,physical_therapy_order.label_th as physical_therapy_orders_th
            ,physical_therapy_order.label_en as physical_therapy_orders_en

            ,sum(case when month_10 is null then 0 else month_10 end) as month_10
            ,sum(case when month_11 is null then 0 else month_11 end) as month_11
            ,sum(case when month_12 is null then 0 else month_12 end) as month_12
            ,sum(case when month_01 is null then 0 else month_01 end) as month_01
            ,sum(case when month_02 is null then 0 else month_02 end) as month_02
            ,sum(case when month_03 is null then 0 else month_03 end) as month_03
            ,sum(case when month_04 is null then 0 else month_04 end) as month_04
            ,sum(case when month_05 is null then 0 else month_05 end) as month_05
            ,sum(case when month_06 is null then 0 else month_06 end) as month_06
            ,sum(case when month_07 is null then 0 else month_07 end) as month_07
            ,sum(case when month_08 is null then 0 else month_08 end) as month_08
            ,sum(case when month_09 is null then 0 else month_09 end) as month_09

            ,sum(total) as total

        ,employee_print.employee_name as employee_print

from (with physical_therapy_order(code, seq, label_th,label_en) as (
    values ('56001',1,'แผ่นความร้อน','Hydrocollator')
                 ,('56002',2,'กระตุ้นกระแสไฟฟ้า','Electrical Stimulator')
                 ,('56003',3,'การรักษาด้วยคลื่นเสียง','Ultrasound Diathermy')
                 ,('56004',4,'การรักษาด้วยคลื่นสั้น','Short Wave Diathermy')
                 ,('56005',5,'การรักษาด้วยคลื่นเลเซอร์','Laser Therapy')
                 ,('56006',6,'แเช่พาราฟิน','Paraffin Bath')
                 ,('56010',7,'ออกกำลังกาย','Exercise')
                 ,('56102',8,'ดึงคอ','Cervial Traction')
                 ,('56121',9,'ดึงหลัง','Pelvic Traction')
                 ,('56702',10,'การดัดดึงข้อต่อ','Manipulation&Mobilization')
                 ,('56007',11,'การกดจุดและนวด','Massage')
                 ,('56008',12,'การพันเทป','Taping')
                 ,('56701',13,'ยืดกล้ามเนื้อ','Stretch'))
select * from physical_therapy_order)  as physical_therapy_order left join
(select

        b_item.item_number as item_number
        ,t_physical_therapy_order_detail.record_datetime
        ,substr(t_visit.visit_begin_visit_time,6,2)
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '10' then 1 else 0 end as "month_10"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '11' then 1 else 0 end as "month_11"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '12' then 1 else 0 end as "month_12"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '01' then 1 else 0 end as "month_01"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '02' then 1 else 0 end as "month_02"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '03' then 1 else 0 end as "month_03"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '04' then 1 else 0 end as "month_04"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '05' then 1 else 0 end as "month_05"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '06' then 1 else 0 end as "month_06"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '07' then 1 else 0 end as "month_07"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '08' then 1 else 0 end as "month_08"
        ,case when substr(t_visit.visit_begin_visit_time,6,2) = '09' then 1 else 0 end as "month_09"

        ,case when substr(t_visit.visit_begin_visit_time,9,2) <> '' then 1 else 0 end as total


from
            t_physical_therapy_order_detail inner join t_physical_therapy_order
                        on t_physical_therapy_order_detail.t_physical_therapy_order_id = t_physical_therapy_order.t_physical_therapy_order_id
                        and t_physical_therapy_order.active = '1'
            inner join b_item on t_physical_therapy_order.b_item_id =b_item.b_item_id
            inner join t_visit on t_physical_therapy_order_detail.t_visit_record_id = t_visit.t_visit_id


where
            b_item.item_number in( '56001', '56002', '56003', '56004', '56005', '56006', '56010', '56102', '56121', '56702', '56007', '56008', '56701')
            and item_active = '1'
            and t_visit.f_visit_status_id <> '4'
            and t_physical_therapy_order_detail.status = '1'
            and substring(t_visit.visit_begin_visit_time,1,7)  between ($P{fiscal_year} -1)||'-10-01' and $P{fiscal_year} ||'-09-30'

) as q
on physical_therapy_order.code = q.item_number

                cross join (select
                                                        b_employee.b_employee_id
                                                        ,case when person_print.t_person_id is not null and prefix_print.f_patient_prefix_id <> '000'
                                                                    then prefix_print.patient_prefix_description||person_print.person_firstname||'  '||person_print.person_lastname
                                                                when person_print.t_person_id is not null and prefix_print.f_patient_prefix_id = '000'
                                                                    then person_print.person_firstname||'  '||person_print.person_lastname
                                                                    else b_employee.employee_firstname||'  '||b_employee.employee_lastname
                                                        end  as employee_name

                                                from b_employee as b_employee left join t_person as person_print on b_employee.t_person_id = person_print.t_person_id
                                                         left join f_patient_prefix as prefix_print on person_print.f_prefix_id = prefix_print.f_patient_prefix_id
                                                where
                                                        b_employee.b_employee_id   = $P{report_by}
                                ) as employee_print

group by
            physical_therapy_seq
            ,physical_therapy_orders_th
            ,physical_therapy_orders_en
            ,employee_print

order by
          physical_therapy_seq asc]]>
	</queryString>
